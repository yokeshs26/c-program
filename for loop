     fact=fact*i;
    printf("%d\n",fact);
}*/
03)
#include<stdio.h>
void main()
{
    int n,fact=0;
    scanf("%d",&n);
    printf("enter the value:");
    for(int i=1;i<n;++i)
     fact=fact+i;
    printf("%d\n",fact);
}
04)
#include<stdio.h>
void main()
{
    int n,fact=0;
    scanf("%d",&n);
    printf("enter the value:");
    for(int i=1;i<n;++i)
      if(i%2==0)
       fact=fact+i;
    printf("%d\n",fact);
}
05)
#include<stdio.h>
void main()
{
    int n,fact=0;
    scanf("%d",&n);
    printf("enter the value:");
    for(int i=1;i<n;++i)
      if(i%2!=0)
       fact=fact+i;
    printf("%d\n",fact);
    }
