#include<stdio.h>
void main()
{
    int n,i,count=0;
    scanf("%d",&n);
    printf("enter the number:");
    for(i=2;i<n;i++)
      if(n%i==0)
      {
      count++;
      break;
      }
      if(count==0)
        printf("it is prime number");
      else
        printf("it is not prime number");
}
