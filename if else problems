01)
#include<stdio.h>
void main()
{
    int a;
    scanf("%d",&a);
    if(a=0){
      printf("the value is 0");
    }
    else 
    if(a>0){
      printf("the value is maximum");
    }
    else {
      printf("the value is minimum");
    }
}

02)
#include<stdio.h>
void main()
{
    int a;
    scanf("%d",&a);
    if(a>0){
      printf("positive");
    }
    else if(a<0){
      printf("negative");
    }
    else{
      printf("default");
    }
}
03)
#include<stdio.h>
void main()
{
    int a,b;
    scanf("%d",&a);
    b=a%2;
    if(b==0)
     printf("the value is even");
    else
     printf("the value is odd");
}
04)
#include<stdio.h>
void main()
{
    int a;
    scanf("%d",&a);
    if(a%4==0||a%400==0)
      printf("leap year");
    else
      printf("not leap year");
}
05)
#include<stdio.h>
void main()
{
    int a,b;
    
    scanf("%d%d",&a,&b);
    if(a==b)
      printf("it is equal");
    else
      printf("it is not equal");
}
06)
#include<stdio.h>
void main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a+b>c&&a+c>b&&b+c>a)
      printf("valid");
    else
      printf("in valid");
}

